{
  "name": "@ibm-skills-network/olx-cli",
  "description": "A command line tool for Open Learning XML",
  "version": "0.1.0-alpha.10",
  "author": "Michael Lin <michael.lin1@ibm.com>",
  "bin": {
    "olx-cli": "./bin/run"
  },
  "bugs": "https://github.com/ibm-skills-network/olx-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^5.0.0",
    "file-type": "^16.5.1",
    "glob": "^7.1.6",
    "got": "^11.8.2",
    "mime-types": "^2.1.31",
    "tar": "^6.1.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "globby": "^10",
    "prettier": "^2.2.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
